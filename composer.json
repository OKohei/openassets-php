{
  "name": "okohei/openassets-php",
  "description": "PHP Bitcoin library with functions for transactions, signatures, serialization, Random/Deterministic ECDSA keys, blocks, RPC bindings",
  "type": "library",
  "license": "Unlicense",
  "authors": [
    {
      "name": "Kohei Ogawa",
      "role": "Author"
    }
  ],
  "autoload": {
    "psr-4": {
      "OKohei\\OpenAssets\\": "src/"
    },
    "files": ["src/Script/functions.php"]
  },
  "autoload-dev": {
    "psr-4": {
      "OKohei\\OpenAssets\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.0",
    "guzzlehttp/guzzle": "~6.0",
    "thefox/utilities": "~1.10",
    "aws/aws-sdk-php": "3.*",
    "bitwasp/bitcoin": "v1.0.2"
  },
  "require-dev": {
    "phpunit/phpunit": "5.5.4"
  }
}
